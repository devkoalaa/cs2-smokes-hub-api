{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/**",
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist"
}
